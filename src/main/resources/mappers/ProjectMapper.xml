<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.green.project_1.project.ProjectMapper">

    <update id="userLock">
        UPDATE user_project
        SET `lock`='1'
        WHERE user_no=#{targetUserNo}
    </update>
</mapper>